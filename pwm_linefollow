prev_err =0 
kp = 2; 
kd = 2; 
lthrust=1500
rthrust=1500
def pid(err):
    
    global prev_err
    derr = err - prev_err
    prev_err = err
    corr = kp * err + kd * derr
    
    pwm2=rthrust+corr#lthrust is the default value of thruster
    pwm1=lthrust-corr
    pwm3=1500
    pwm4=1500
    pwm5=1500
    pwm6=1500
    print(pwm1)
    print(pwm2)
